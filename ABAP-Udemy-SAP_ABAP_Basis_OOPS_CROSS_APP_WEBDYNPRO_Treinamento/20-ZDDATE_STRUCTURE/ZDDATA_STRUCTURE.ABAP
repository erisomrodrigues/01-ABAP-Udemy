REPORT ZDDATA_STRUCTURE.
DATA: BEGIN OF ST3,
      FIRST       TYPE I,
      SECOND(20)  TYPE C,
  BEGIN OF ST4,
      4FISRT      TYPE I,
      4SECOND(20) TYPE C,
  END OF ST4,
      THIRD       TYPE STRING,
  END OF ST3.

  ST3-FIRST = 1000.
  ST3-SECOND = 'HELLO'.
  ST3-ST4-4FISRT = 'BYE'.
  ST3-ST4-4SECOND = 'THIRD FIELD'.

  WRITE:/ 'VALUE FOR ST3-FISRT IS ', ST3-FIRST,
        / 'VALUE FOR ST3-SECOND IS ', ST3-SECOND,
        / 'VALUE FOR ST3-THIRD IS ', ST3-ST4-4FISRT.
